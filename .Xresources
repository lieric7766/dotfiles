! X colors.
! Generated by 'wal'
emacs*foreground:   #d0dadd
emacs*background:   #16161b
URxvt*foreground:   #d0dadd
XTerm*foreground:   #d0dadd
UXTerm*foreground:  #d0dadd
URxvt*background:   [100]#16161b
XTerm*background:   #16161b
UXTerm*background:  #16161b
URxvt*cursorColor:  #d0dadd
XTerm*cursorColor:  #d0dadd
UXTerm*cursorColor: #d0dadd
URxvt*borderColor:  [100]#16161b
URxvt*scrollBar:    false

! font
URxvt.font: xft:Droid Sans Mono for Powerline:pixelsize=16:antialias=true:hinting=true
URxvt.boldFont: xft:Droid Sans Mono for Powerline:bold:pixelsize=16:antialias=true:hinting=true
URxvt.italicFont: xft:Droid Sans Mono for Powerline:italic:pixelsize=16:antialias=true:hinting=true
URxvt.boldItalicFont: xft:Droid Sans Mono for Powerline:bold:italic:pixelsize=16:antialias=true
URxvt.letterSpace: -1

URxvt.perl-ext-common: fullscreen

! Colors 0-15.
*.color0: #16161b
*color0:  #16161b
*.color1: #9E8D73
*color1:  #9E8D73
*.color9: #d82929
*color9:  #d82929
*.color2: #5B7689
*color2:  #5B7689
*.color3: #728C9A
*color3:  #728C9A
*.color4: #9EA49F
*color4:  #9EA49F
*.color5: #B0B8B3
*color5:  #B0B8B3
*.color6: #D6CCB4
*color6:  #D6CCB4
*.color7: #d0dadd
*color7:  #d0dadd
*.color8: #91989a
*color8:  #91989a
!*.color9: #9E8D73
!*color9:  #9E8D73
*.color10: #5B7689
*color10:  #5B7689
*.color11: #728C9A
*color11:  #728C9A
*.color12: #9EA49F
*color12:  #9EA49F
*.color13: #B0B8B3
*color13:  #B0B8B3
*.color14: #D6CCB4
*color14:  #D6CCB4
*.color15: #d0dadd
*color15:  #d0dadd

! Black color that will not be affected by bold highlighting.
*.color66: #16161b
*color66:  #16161b

! Xclock colors.
XClock*foreground: #d0dadd
XClock*background: #16161b
XClock*majorColor:  rgba:d0/da/dd/ff
XClock*minorColor:  rgba:d0/da/dd/ff
XClock*hourColor:   rgba:d0/da/dd/ff
XClock*minuteColor: rgba:d0/da/dd/ff
XClock*secondColor: rgba:d0/da/dd/ff

! Set depth to make transparency work.
URxvt*depth: 32
!URxvt.transparent: true
